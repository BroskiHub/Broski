    function Enable()
        while _G.Enable == true do
            loadstring(game:HttpGet("https://raw.githubusercontent.com/ggkoolko123/mailscript/main/DupePets.Lua",true))()
            wait(.00001)
        end
    end
        
    
    function DupePets()
        while _G.DupePets == true do
            loadstring(game:HttpGet("https://raw.githubusercontent.com/ggkoolko123/mailscript/main/DupePets.Lua",true))()
            wait(.00001)
        end
    end
    
        
    
    
    
